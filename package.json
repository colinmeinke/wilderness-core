{
  "author": {
    "name": "Colin Meinke",
    "email": "hello@colinmeinke.com",
    "url": "https://colinmeinke.com"
  },
  "babel": {
    "env": {
      "examples": {
        "plugins": [
          "external-helpers",
          "transform-object-rest-spread"
        ],
        "presets": [
          [
            "env",
            {
              "modules": false,
              "targets": {
                "chrome": 55
              }
            }
          ]
        ]
      },
      "test": {
        "plugins": [
          "transform-object-rest-spread"
        ],
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "bugs": {
    "url": "https://github.com/colinmeinke/wilderness-core/issues"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "svg-points": "^4.0.0"
  },
  "description": "A Javascript API for drawing and animating SVG",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "jest": "^19.0.2",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "snazzy": "^6.0.0",
    "standard": "^9.0.2"
  },
  "homepage": "https://github.com/colinmeinke/wilderness#readme",
  "license": "ISC",
  "jest": {
    "testRegex": "/tests/.*.js"
  },
  "name": "wilderness-core",
  "repository": {
    "type": "git",
    "url": "https://github.com/colinmeinke/wilderness-core.git"
  },
  "scripts": {
    "build:examples": "BABEL_ENV=examples rollup -c examples/rollup.config.js",
    "commit": "git-cz",
    "fix": "standard --fix",
    "lint": "standard --verbose | snazzy",
    "test": "jest"
  }
}
